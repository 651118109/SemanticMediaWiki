{
	"description": "Test output of `Special:SearchByProperty` (`wgContLang=en`, skip-on sqlite, 1.19)",
	"properties": [
		{
			"name": "Has text",
			"contents": "[[Has type::Text]]"
		},
		{
			"name": "Has number",
			"contents": "[[Has type::Number]] [[Display precision of::2]]"
		}
	],
	"subjects": [
		{
			"name": "Example/S0002/1",
			"contents": "[[Has text::S0002]]"
		},
		{
			"name": "Example/S0002/2",
			"contents": "[[Has text::foo bar]]"
		},
		{
			"name": "Example/S0002/3",
			"contents": "[[Has number::3.555567]]"
		},
		{
			"name": "Example/S0002/4",
			"contents": "[[Has number::1.2e-13]]"
		}
	],
	"special-testcases": [
		{
			"about": "#0 search for user-defined property to contain",
			"special-page": {
				"page":"SearchByProperty",
				"query-parameters": "",
				"request-parameters":{
					"property": "Has text"
				}
			},
			"expected-output": {
				"to-contain": [
					"<ul><li>S0002&#160;&#160;",
					"title=\"Special:SearchByProperty/Has-20text/S0002\">+</a></span>"
				]
			}
		},
		{
			"about": "#1 value to contain `_`",
			"special-page": {
				"page":"SearchByProperty",
				"query-parameters": "",
				"request-parameters":{
					"property": "Has text",
					"value": "foo_bar"
				}
			},
			"expected-output": {
				"to-contain": [
					"Example-2FS0002-2F2",
					"<small>(foo bar)</small>"
				]
			}
		},
		{
			"about": "#2 value to contain ` `",
			"special-page": {
				"page":"SearchByProperty",
				"query-parameters": "",
				"request-parameters":{
					"property": "Has text",
					"value": "foo bar"
				}
			},
			"expected-output": {
				"to-contain": [
					"Example-2FS0002-2F2",
					"<small>(foo bar)</small>"
				]
			}
		},
		{
			"about": "#3 value input not be constraint by precision",
			"special-page": {
				"page":"SearchByProperty",
				"query-parameters": "",
				"request-parameters":{
					"property": "Has number",
					"value": "3.555567"
				}
			},
			"expected-output": {
				"to-contain": [
					"Example-2FS0002-2F3",
					"<small>(3.556)</small>"
				],
				"not-contain": [
					"value=3.56"
				]
			}
		},
		{
			"about": "#4 do not encode e- for a number value",
			"special-page": {
				"page":"SearchByProperty",
				"query-parameters": "",
				"request-parameters":{
					"property": "Has number",
					"value": "1.2e-13"
				}
			},
			"expected-output": {
				"to-contain": [
					"Example-2FS0002-2F4",
					"<small>(1.2e-13)</small>"
				]
			}
		},
		{
			"about": "#5 same as 1.2e-13",
			"special-page": {
				"page":"SearchByProperty",
				"query-parameters": "",
				"request-parameters":{
					"property": "Has number",
					"value": "0.00000000000012"
				}
			},
			"expected-output": {
				"to-contain": [
					"Example-2FS0002-2F4",
					"<small>(1.2e-13)</small>"
				]
			}
		}
	],
	"settings": {
		"wgContLang": "en",
		"wgLanguageCode": "en"
	},
	"meta": {
		"skip-on": {
			"sqlite": "Returns a `database is locked`",
			"mw-1.19.20": "The table update on 1.19 lacks behind"
		},
		"version": "0.1",
		"is-incomplete": false,
		"debug": false
	}
}